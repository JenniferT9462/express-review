<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>
    <link rel="stylesheet" 
    type="text/css" 
    href="/public/css/styles.css">
  </head>
  <body>
    <%- include('partials/header') %>
    <%- include('partials/navBar') %>
    <h1><%= title %></h1>
    <p><%= message %></p>
    <h2>Users:</h2>
    <ul>
      <% users.forEach((user) => { %>
      <li>
        Id: <%= user.id %> <br> User Name: <%= user.username %> <br> Active: <%=
        user.isActive %> <br> Email: <%= user.email %>
      </li>
      <% }); %>
    </ul>

    <%- include('partials/footer') %>
  </body>
</html>
